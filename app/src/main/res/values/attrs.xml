<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ArcProgressView">
        <attr name="arcBgColor" format="color" />
        <attr name="arcProgressColor" format="color" />
        <attr name="arcSize" format="dimension" />
        <attr name="arcAnimDuration" format="integer" />
    </declare-styleable>


    <style name="CircleStyle">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>


    <declare-styleable name="CodeEditText">
        <!--code模式-->
        <attr name="code_mode" format="enum">
            <!--文字-->
            <enum name="text" value="0" />
            <!--TODO 拓展-->
        </attr>

        <!--code样式-->
        <attr name="code_style" format="enum">
            <!--表格-->
            <enum name="form" value="0" />
            <!--方块-->
            <enum name="rectangle" value="1" />
            <!--横线-->
            <enum name="line" value="2" />
            <!--圆形-->
            <enum name="circle" value="3" />
            <!--TODO 拓展-->
        </attr>

        <!--code背景色-->
        <attr name="code_bg_color" format="color" />
        <!--边框宽度-->
        <attr name="code_border_width" format="dimension" />
        <!--边框默认颜色-->
        <attr name="code_border_color" format="color" />
        <!--边框选中颜色-->
        <attr name="code_border_select_color" format="color" />
        <!--边框圆角-->
        <attr name="code_border_radius" format="dimension" />
        <!--code 内容颜色（密码或文字）-->
        <attr name="code_content_color" format="color" />
        <!--code 内容大小（密码或文字）-->
        <attr name="code_content_size" format="dimension" />
        <!--code 单个宽度-->
        <attr name="code_item_width" format="dimension" />
        <!--code Item之间的间隙-->
        <attr name="code_item_space" format="dimension" />

    </declare-styleable>


    <attr name="icvp_min_page_scale_offset" format="dimension" />
    <attr name="icvp_center_page_scale_offset" format="dimension" />
    <attr name="icvp_min_page_scale" format="float" />
    <attr name="icvp_max_page_scale" format="float" />
    <attr name="icvp_medium_scaled" format="boolean" />
    <attr name="icvp_scroll_duration" format="integer" />
    <attr name="icvp_page_duration" format="integer" />
    <attr name="icvp_interpolator" format="reference" />
    <declare-styleable name="HorizontalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset" />
        <attr name="icvp_center_page_scale_offset" />
        <attr name="icvp_min_page_scale" />
        <attr name="icvp_max_page_scale" />
        <attr name="icvp_medium_scaled" />
        <attr name="icvp_scroll_duration" />
        <attr name="icvp_page_duration" />
        <attr name="icvp_interpolator" />
    </declare-styleable>

    <declare-styleable name="VerticalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset" />
        <attr name="icvp_center_page_scale_offset" />
        <attr name="icvp_min_page_scale" />
        <attr name="icvp_max_page_scale" />
        <attr name="icvp_medium_scaled" />
        <attr name="icvp_scroll_duration" />
        <attr name="icvp_page_duration" />
        <attr name="icvp_interpolator" />
    </declare-styleable>


    <declare-styleable name="TriangleRatingAnimView">
        <attr name="maxRating" format="integer" />
        <attr name="upRating" format="integer" />
        <attr name="leftRating" format="integer" />
        <attr name="rightRating" format="integer" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="ratingStrokeColor" format="color" />
        <attr name="ratingStrokeWidth" format="dimension" />
        <attr name="enableAnimation" format="boolean" />
        <attr name="circleRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxLines" format="integer" />
        <attr name="expandDrawable" format="reference" />
        <attr name="collapseDrawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="SvExpandableTextView">
        <attr name="sv_expand_text" format="string" />
        <attr name="sv_shrink_text" format="string" />
        <attr name="sv_expand_show_shrink" format="boolean" />
        <attr name="sv_expand_color_shrink" format="color" />
        <attr name="sv_expand_color_expand" format="color" />
        <attr name="sv_expand_lineSpacingMultiplier" format="float" />
        <attr name="sv_expand_lineSpacingExtra" format="dimension" />
        <attr name="sv_expand_link_clickable" format="boolean" />
        <attr name="sv_expand_icon" format="reference" />
        <attr name="sv_shrink_icon" format="reference" />
        <attr name="sv_expand_style" format="enum">
            <enum name="text" value="1" />
            <enum name="image" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="LimitHeightRecyclerView">
        <attr name="limitHeight" format="dimension" />
    </declare-styleable>
</resources>